{
  "name": "root",
  "version": "0.0.0",
  "packageManager": "pnpm@9.14.4",
  "type": "module",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/**/*"
    ],
    "catalog": {
      "typescript": "^5.9.3"
    },
    "catalogs": {
      "testing": {
        "vitest": "^4.0.16"
      },
      "effect": {
        "effect": "3.19.12",
        "@effect/platform": "0.94.1",
        "@effect/platform-bun": "0.86.0",
        "@effect/platform-browser": "0.73.0",
        "@effect/platform-node": "0.103.0",
        "@effect/rpc": "0.72.2",
        "@effect/experimental": "0.58.0",
        "@effect/language-service": "^0.64.1",
        "@effect/sql": "0.49.0",
        "@effect/sql-pg": "0.49.7",
        "@effect/sql-kysely": "0.45.0",
        "@effect/cluster": "0.56.1",
        "@effect/opentelemetry": "0.60.0",
        "@effect/workflow": "0.16.0",
        "@effect-atom/atom": "0.4.11",
        "@effect/ai": "0.33.2",
        "@effect/ai-openrouter": "0.8.0",
        "@effect/vitest": "0.27.0"
      }
    }
  },
	"scripts": {
		"prepare": "effect-language-service patch",
		"clean": "pnpm -r clean && rm -rf node_modules",
		"build": "pnpm -r build",
		"dev": "pnpm -r dev",
		"typecheck": "pnpm -r typecheck",
		"check": "pnpm typecheck",
		"test": "pnpm -r test",
		"test:e2e": "playwright test",
		"lint": "pnpm -r lint",
		"format": "prettier -w .",
		"format:check": "prettier -c .",
		"changeset": "changeset",
		"changeset:version": "changeset version",
    "changeset:status": "changeset status --since=main"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@effect/language-service": "^0.69.1",
    "@eslint/js": "^9.26.0",
    "@types/node": "^25.0.9",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.26.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-simple-import-sort": "^12.1.0",
		"globals": "^15.15.0",
		"@playwright/test": "^1.55.0",
		"prettier": "^3.6.2",
		"turbo": "^2.5.5",
		"typescript": "^5.9.3",
		"vitest": "catalog:testing"
	}
